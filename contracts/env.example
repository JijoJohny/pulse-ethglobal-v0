# Rootstock Network Configuration
PRIVATE_KEY=your_private_key_here

# Optional: Fee recipient for mainnet deployment
FEE_RECIPIENT=your_fee_recipient_address_here

# Optional: Gas reporting
REPORT_GAS=true

# Optional: Rootstock API key for contract verification
ROOTSTOCK_API_KEY=your_rootstock_api_key_here

# Network RPC URLs (for reference)
# Rootstock Testnet: https://public-node.testnet.rsk.co
# Rootstock Mainnet: https://public-node.rsk.co

# Network Chain IDs (for reference)
# Rootstock Testnet: 31
# Rootstock Mainnet: 30

# Gas Price (in wei) - Rootstock typically uses 0.06 gwei
GAS_PRICE=60000000

# Contract Addresses (will be populated after deployment)
# POSITION_CONTRACT_ADDRESS=
# MARKET_CORE_ADDRESS=
